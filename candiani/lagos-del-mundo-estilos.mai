.woocommerce ul.products li.product a {text-decoration:underline}
a.button, a.tp-button, button, input[type="submit"], input[type="reset"], input[type="button"] {border-radius:0px;}
button.single_add_to_cart_button:hover{
	background-color:#fff;
	/*js*/
}
.woocommerce .product div.entry-summary h1.product_title{
	border-bottom:0px;
}
.social_buttons_wrapper a:hover{
	padding-bottom: 3px;
}
aside h3{border-bottom:1px solid #000000;}
.tnp-subscription input[type="text"], .tnp-subscription input[type="email"], .tnp-subscription input[type="submit"], .tnp-subscription select{
	background-color:#FFFFFF;
	border:1px solid #CCCCCC;
	margin-right:10px;
}
.tnp-subscription input.tnp-submit{
	background-color:#FFFFFF !important;
	color:#000000;
	display: block;
padding: 4px 20px;
	
}
.social_buttons_wrapper .tnp-subscription{margin-top:10px;}

.woocommerce .product div.entry-summary .cart .button:hover{background-color:#BF8C3D !important;}

